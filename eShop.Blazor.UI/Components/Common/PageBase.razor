@using eShop.Blazor.Application.Routing
@using eShop.Blazor.Application.State
@using eShop.Blazor.Infrastructure.Security
@inherits ComponentBase

@code {
    [Inject] public RouteManager RouteManager { get; private set; } = null!;
    [Inject] public AuthenticationManager AuthenticationManager { get; private set; } = null!;
    [Inject] public UserState UserState { get; private set; } = null!;
    [Inject] public ISnackbar Snackbar { get; private set; } = null!;
    [Inject] public IJSRuntime Js { get; private set; } = null!;
    [Inject] public IStorage LocalStorage { get; private set; } = null!;
}
